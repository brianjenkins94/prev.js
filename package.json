{
  "name": "prev",
  "version": "1.0.0",
  "description": "The React framework for people who don't want to learn React.",
  "bin": "scripts/npx.js",
  "type": "module",
  "scripts": {
    "cypress": "cypress open",
    "build": "(next build && kill -INT 0 & npm run hack)",
    "hack": "while sleep 1; do echo \"{\n  \\\"type\\\": \\\"CommonJS\\\"\n}\" > .next/package.json; done",
    "start": "(node --experimental-specifier-resolution=node --loader=ts-node/esm server.ts && kill -INT 0 && npm run hack)",
    "test": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjenkins94/prev.git"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianjenkins94/prev/issues"
  },
  "homepage": "https://github.com/brianjenkins94/prev#readme",
  "dependencies": {
    "eta": "latest",
    "juvy": "latest",
    "next": "canary",
    "react": "latest",
    "react-dom": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^7.32.0",
    "eslint-config-next": "latest"
  }
}
